    "mode-watcher": "^0.3.0",
    "paneforge": "^0.0.2",
    "postprocessing": "^6.35.3",
    "svelte-sonner": "^0.3.21",
    "sveltekit-superforms": "^2.12.2",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1",
    "three": "^0.163.0",
    "vaul-svelte": "^0.3.0",
    "zod": "^3.22.4",
    "shared": "workspace:*"
  }
}
